# Production Environment

EXPO_PUBLIC_ENV=production

# Production API (Update with your actual production URL)
EXPO_PUBLIC_API_URL=https://api.aiquizapp.com
EXPO_PUBLIC_WS_URL=wss://api.aiquizapp.com

# Third-party Services (Add your production keys)
EXPO_PUBLIC_SENTRY_DSN=${SENTRY_DSN}
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=${GOOGLE_MAPS_API_KEY}
EXPO_PUBLIC_ONESIGNAL_APP_ID=${ONESIGNAL_APP_ID}
EXPO_PUBLIC_AMPLITUDE_API_KEY=${AMPLITUDE_API_KEY}

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true